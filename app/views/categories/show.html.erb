

<p>
  <strong>Name:</strong>
  <%= @category.name %>
</p>

<%@rooms.each do |room|%>
  <%if room.category_id == @category.id %>
      <%= link_to room.name, mall_room_path(room)%>
      <p>located at <%= link_to room.mall.name, mall_path%></p>
  <%end%>
<%end%>

<%= link_to 'Edit', edit_category_path(@category) %> |
<%= link_to 'Back', categories_path %>
